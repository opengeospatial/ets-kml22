<?xml version="1.0" encoding="UTF-8"?>
<kml:PhotoOverlay xmlns:kml="http://www.opengis.net/kml/2.2">
  <kml:Icon>
    <kml:href>./L$[level]/row-$[x]_col-$[y].jpg</kml:href>
  </kml:Icon>
  <kml:ImagePyramid>
    <kml:tileSize>256</kml:tileSize>
    <kml:maxWidth>1024</kml:maxWidth>
    <kml:maxHeight>1024</kml:maxHeight>
    <kml:gridOrigin>lowerLeft</kml:gridOrigin>
  </kml:ImagePyramid>
</kml:PhotoOverlay>
